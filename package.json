  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.4",
    "istanbul": "^0.4.5",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  },
  "dependencies": {}
}
