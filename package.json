{
  "name": "double.js",
  "version": "0.4.2",
  "description": "Floating point expansions with 31 accurate decimal digits (100+ bits), also known as double-word arithmetic",
  "main": "dist/double.js",
  "types": "dist/double.d.ts",
  "module": "dist/double.esm.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"npm:serve\"",
    "test": "node --experimental-modules --no-warnings ./test/puppeteer.js",
    "ieee": "node ./test/float-paranoia.js",
    "serve": "http-server -c-1 -p 1234",
    "ci": "concurrently -k -s first \"npm:test\" \"npm:serve\"",
    "cov": "nyc report --reporter=lcov",
    "deploy": "npm version minor && git push --tags origin master"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "codecov": "3.6.1",
    "nyc": "^14.1.1",
    "puppeteer": "^2.0.0",
    "puppeteer-to-istanbul": "^1.2.2",
    "rollup": "1.27.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-typescript2": "^0.25.2",
    "typescript": "^3.7.2",
    "zora": "^3.1.5"
  },
  "eslintIgnore": [
    "dist",
    "test"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "keywords": [
    "float128",
    "quadruple",
    "double-double",
    "gmp",
    "mpfr",
    "bigfloat",
    "bigdecimal",
    "bignumber.js",
    "decimal.js",
    "big.js"
  ],
  "author": "munrocket",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.0.0"
  }
}
