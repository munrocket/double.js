{
  "name": "double.js",
  "version": "0.4.1",
  "description": "Floating point expansions with 31 accurate decimal digits (100+ bits), also known as double-word arithmetic",
  "main": "dist/double.js",
  "types": "dist/double.d.ts",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "serve": "http-server -p 1234 -c-1",
    "ci": "npm run build && npm run coverage",
    "test": "node ./test/test.js",
    "coverage": "istanbul cover ./test/test.js --report lcovonly -- -R spec",
    "deploy": "npm version patch && git push --tags origin master",
    "float-paranoia": "node ./test/paranoia.js"
  },
  "eslintIgnore": [
    "dist",
    "test"
  ],
  "keywords": [
    "float128",
    "quadruple",
    "double-double",
    "gmp",
    "mpfr",
    "bigfloat",
    "bigdecimal",
    "bignumber.js",
    "decimal.js",
    "big.js"
  ],
  "author": "munrocket",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "codecov": "3.6.1",
    "istanbul": "0.4.5",
    "rollup": "1.27.4",
    "rollup-plugin-babel": "4.3.3",
    "tap-spec": "5.0.0",
    "tape": "4.11.0"
  }
}
